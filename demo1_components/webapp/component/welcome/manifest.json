{
    "_version": "1.4.0",
    "sap.app": {
        "id": "welcome",
        "type": "application",
        "title": "Welcome",
        "description": "Main page component",
        "applicationVersion": {
            "version": "1.0.0"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://detail-view",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_hcb",
            "sap_bluecrystal"
        ]
    },
    "sap.ui5": {
        "rootView": {
            "viewName": "welcome.view.Main",
            "type": "XML"
        },
        "dependencies": {
            "minUI5Version": "1.42.0",
            "libs": {
                "sap.ui.core": {
                    "minVersion": "1.42.0"
                },
                "sap.m": {
                    "minVersion": "1.42.0"
                }
            }
        },
        "componentUsages": {
			"book": {
                "name": "book",
                "manifest": true
			}
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
        },
        "resources": {
            "js": [
            ]
        },
        "routing": {
          "config": {
              "routerClass": "sap.m.routing.Router",
              "viewType": "XML",
              "viewPath": "shop.view",
              "async": true
          },
          "routes": [
              {
                  "pattern": "ExtraInComponentRoute",
                  "name": "Main"
              }
          ]
        }
    }
}
